Sum of the Numbers in a String
#include<iostream>
using namespace std;
#include<bits/stdc++.h>
void x(string m){
    int total=0;
    // int b=0;
    // int c=0;
    // string mm;
    string l="";
    for(int i=0; i<m.length(); i++){
        
   
        if(m[i]>='0'&&m[i]<='9' ){
            l.push_back(m[i]);
            
        }
        else {
            if(!l.empty()){
                total+=stoi(l);
                l="";
            }
        
        }
    }
 
      if (!l.empty()) {  // Add the last number if it exists
        total += stoi(l);
    }
    cout<<total;
    
}
int main(){

    x("1xyz23");
    
}

